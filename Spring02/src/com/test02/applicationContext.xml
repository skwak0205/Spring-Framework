<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean으로 2명 등록 -->
	<bean id="hong" class="com.test02.Address">
		<constructor-arg name="name" value="홍길동" />
		<constructor-arg name="addr" value="서울 구로구" />
		<constructor-arg name="phone" value="010-1111-1111" />
	</bean>
	
	<bean id="kim" class="com.test02.Address">
		<constructor-arg value="김선비" />
		<constructor-arg value="경기도 부천" />
		<constructor-arg value="010-9999-9999" />
	</bean>

</beans>
